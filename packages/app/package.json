{
  "name": "@starter/app",
  "version": "0.1.0",
  "homepage": "https://zkindest.github.io/starter-fullstack-graphql-rq/",
  "dependencies": {
    "broadcast-channel": "^4.10.0",
    "graphql": "^16.3.0",
    "jwt-decode": "^3.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.16",
    "react-router-dom": "6",
    "web-vitals": "2.1.4",
    "zustand": "^3.7.1"
  },
  "scripts": {
    "download:schema:local": "rover graph introspect http://localhost:4000 > src/__generated__/schema.graphql",
    "download:schema": "rover graph fetch starter-fullstack@current > src/__generated__/schema.graphql",
    "gql:codegen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint  --quiet --fix --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write",
    "deploy": "bash ./deploy.sh",
    "msw:init": "msw init public/ --save"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-react-query": "^3.5.7",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@tsconfig/vite-react": "^1.0.0",
    "@types/react": "17.0.41",
    "@types/react-dom": "17.0.12",
    "@vitejs/plugin-react": "1.2.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.0",
    "jsdom": "latest",
    "msw": "0.38.1",
    "vite": "latest",
    "vite-plugin-windicss": "^1.8.3",
    "vitest": "latest",
    "windicss": "^3.5.1"
  }
}
