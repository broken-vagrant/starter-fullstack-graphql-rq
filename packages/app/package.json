{
  "name": "@starter/app",
  "version": "0.1.0",
  "homepage": "https://zkindest.github.io/react-starter",
  "dependencies": {
    "apollo": "^2.33.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "download-schema": "apollo service:download --endpoint=http://localhost:4000/ graphql-schema.json",
    "codegen": "npm run download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript ",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint  --quiet --fix --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write",
    "precommit": "yarn run prettier"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/client": "3.5.10",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.12",
    "@vitejs/plugin-react": "1.2.0",
    "cross-fetch": "3.1.5",
    "jsdom": "latest",
    "msw": "0.38.1",
    "vite": "latest",
    "vitest": "latest"
  }
}
